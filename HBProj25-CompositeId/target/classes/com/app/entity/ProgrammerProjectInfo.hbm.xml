<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

	<hibernate-mapping>
	 <class name="com.app.entity.ProgrammerProjectInfo" table="pp_info">
	   
	    <composite-id name="id" class="com.app.entity.ProgrammerProjectId">
              <key-property name="projId" />
              <key-property name="pid" />	    
	    </composite-id>
	    <property name="projectName"  />
	    <property name="programmerName" />
	       
	 
	 </class>
	
	</hibernate-mapping>